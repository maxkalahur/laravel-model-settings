{
    "name": "maxkalahur/laravel-model-settings",
    "description": "This Laravel package allows you to attach settings to Laravel Models, save them in DB and encrypt.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "settings",
        "custom properties",
        "model settings",
        "model properties",
        "model attributes",
        "model metadata",
        "model options",
        "model configuration",
        "model custom fields",
        "model custom attributes",
        "model custom properties",
        "model custom settings",
        "model custom options",
        "model custom metadata",
        "model custom configuration",
        "model custom fields"
    ],
    "authors": [
        {
            "name": "Maksym Kalahur",
            "email": "maxkalahur@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.2",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/encryption": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11",
        "orchestra/testbench": "^10"
    },
    "autoload": {
        "psr-4": {
            "MaxKalahur\\LaravelModelSettings\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MaxKalahur\\LaravelModelSettings\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MaxKalahur\\LaravelModelSettings\\CustomSettingServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
